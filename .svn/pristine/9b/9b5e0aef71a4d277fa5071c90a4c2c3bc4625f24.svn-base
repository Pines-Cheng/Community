<!DOCTYPE html>
<html ng-app="manage" ng-controller="manageController">

<head>
    <title>新格投资后台管理系统</title>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <meta name="format-detection" content="telephone=no">
    <!-- 指定网页使用 webkit 引擎渲染 -->
    <meta name="renderer" content="webkit">
    <!-- 防止百度转码后贴广告 -->
    <meta http-equiv="Cache-Control" content="no-siteapp" />
    <!-- CSS -->
    <link rel="stylesheet" href="../css/amazeui/amazeui.min.css" />
    <link rel="stylesheet" type="text/css" href="../css/amazeui/admin.css">
    <link rel="stylesheet" href="../css/common.css" />
</head>
<!-- 定义ng-app -->

<body style="min-height:880px" ng-app="manage">
    <!-- 页眉 -->
    <header class="am-topbar admin-header">
        <div class="am-topbar-brand ">
            <img style="width:30px" src="../img/LOGO-05.png" alt="logo">
            <strong>大鱼</strong> <small>后台管理系统</small>
        </div>
        <div class="am-collapse am-topbar-collapse" id="topbar-collapse">
            <ul class="am-nav am-nav-pills am-topbar-nav am-topbar-right admin-header-list">
                <li><a href="javascript:;"><span class="am-icon-envelope-o"></span> 收件箱 <span class="am-badge am-badge-warning"></span></a></li>
                <li class="am-dropdown" data-am-dropdown>
                    <a class="am-dropdown-toggle" data-am-dropdown-toggle href="javascript:;">
                        <span class="am-icon-users"></span> 管理员 <span class="am-icon-caret-down"></span>
                    </a>
                    <ul class="am-dropdown-content">
                        <li><a href="#"><span class="am-icon-user"></span> 资料</a></li>
                        <li><a href="#"><span class="am-icon-cog"></span> 设置</a></li>
                        <li><a href="#"><span class="am-icon-power-off"></span> 退出</a></li>
                    </ul>
                </li>
            </ul>
        </div>
    </header>
    <div class="am-cf admin-main" style="height:100%">
        <!-- 左侧边栏菜单 -->
        <div ng-include=" 'leftMenu.html' "></div>
        <!-- 内容栏 -->
        <div class="admin-content">
            <!-- 路径引导 -->
            <div class="am-cf am-padding">
                <div class="am-fl am-cf"><strong class="am-text-primary am-text-lg" ng-bind="pathTips.firstTips"></strong> / <small ng-bind="pathTips.secondTips"></small></div>
            </div>
            
            <!-- 使用ng-view管理 -->
            <div ui-view='content'></div>
        </div>
    </div>
</body>
<!-- lib -->
<script type="text/javascript" src="../js/lib/jquery/jquery.min.js"></script>
<!-- angularJS -->
<script type="text/javascript" src="../js/lib/angularjs/angular.js"></script>
<script type="text/javascript" src="../js/lib/angularjs/angular-route.js"></script>
<script type="text/javascript" src="../js/lib/angularjs/angular-animate.js"></script>
<!-- amazeui -->
<script type="text/javascript" src="../js/lib/amazeui/amazeui.min.js"></script>
<!-- angular-ui -->
<script type="text/javascript" src="../js/lib/angularui/angular-ui-router.js"></script>
<!--  -->
<!-- my lib -->
<script type="text/javascript" src="../js/myangular/controller.js"></script>
<script type="text/javascript" src="../js/myangular/filter.js"></script>
<script type="text/javascript" src="../js/myangular/service.js"></script>
<script type="text/javascript" src="../js/myangular/directive.js"></script>
<script type="text/javascript" src="../js/myangular/app.js"></script>

</html>
